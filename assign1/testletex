let x = 1 in x + 1
let x = 1 in let y = 2 in x * y
let x = 10 in let y = 20 in x * y
let x = 5 in let y = let z = 2 in z + x in y * x
let x = 0 in let y = 1 in x / y
let x = 1 in let x = 2 in x + x
let x = 5 in let y = let z = 10 in z * y in x + y
let x = 1 in let y = x + 1 in let z = y * 2 in z - x
let x = 1 in let y = 0 in let z = x / (y + 1) in z
let x = 100 in let y = x / 2 in let z = y - 10 in z * 2
